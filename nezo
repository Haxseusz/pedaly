    <script>
        const DISCORD_USER_ID = '407656357844746240';
        const bgMusic = document.getElementById('bgMusic');
        const musicControl = document.getElementById('musicControl');
        const playIcon = document.getElementById('playIcon');
        const pauseIcon = document.getElementById('pauseIcon');
        const blurOverlay = document.getElementById('blurOverlay');
        let isPlaying = false;


        blurOverlay.addEventListener('click', () => {
            bgMusic.volume = 0.3;
            bgMusic.play().then(() => {
                isPlaying = true;
                pauseIcon.style.display = 'block';
                playIcon.style.display = 'none';
                musicControl.classList.add('playing');
                blurOverlay.classList.add('hidden');
                setTimeout(() => {
                    blurOverlay.remove();
                }, 500);
            }).catch((err) => {
                console.log('Błąd odtwarzania:', err);
                blurOverlay.classList.add('hidden');
            });
        });


        musicControl.addEventListener('click', () => {
            if (isPlaying) {
                bgMusic.pause();
                playIcon.style.display = 'block';
                pauseIcon.style.display = 'none';
                musicControl.classList.remove('playing');
            } else {
                bgMusic.play();
                playIcon.style.display = 'none';
                pauseIcon.style.display = 'block';
                musicControl.classList.add('playing');
            }
            isPlaying = !isPlaying;
        });


        async function fetchDiscordProfile() {
            try {

                const response = await fetch(`https://api.lanyard.rest/v1/users/${DISCORD_USER_ID}`);

                if (!response.ok) {
                    throw new Error('Nie udało się pobrać danych');
                }

                const result = await response.json();
                
                if (result.success && result.data) {
                    updateProfile(result.data);
                } else {
                    throw new Error('Brak danych użytkownika');
                }
            } catch (error) {
                console.error('Błąd pobierania profilu Discord:', error);

                updateProfile({
                    discord_user: {
                        username: 'DiscordUser',
                        discriminator: '0',
                        avatar: null,
                        id: DISCORD_USER_ID
                    },
                    discord_status: 'offline',
                    activities: []
                });
            }
        }

        function updateProfile(data) {
            const username = document.getElementById('username');
            const avatar = document.getElementById('avatar');
            const bio = document.getElementById('bio');


            const user = data.discord_user || data;
            const status = data.discord_status || 'offline';
            

            const displayName = user.global_name || user.username;
            username.textContent = `@${displayName}`;


            if (user.avatar) {
                const avatarUrl = `https://cdn.discordapp.com/avatars/${user.id || DISCORD_USER_ID}/${user.avatar}.${user.avatar.startsWith('a_') ? 'gif' : 'png'}?size=256`;
                avatar.innerHTML = `<img src="${avatarUrl}" alt="Avatar">`;
            } else {

                const defaultAvatar = (parseInt(user.id || DISCORD_USER_ID) >> 22) % 6;
                avatar.innerHTML = `<img src="https://cdn.discordapp.com/embed/avatars/${defaultAvatar}.png" alt="Avatar">`;
            }


            const statusColors = {
                'online': '#43b581',
                'idle': '#faa61a',
                'dnd': '#f04747',
                'offline': '#747f8d'
            };
            
            if (status) {
                const statusDot = document.createElement('div');
                statusDot.style.cssText = `
                    position: absolute;
                    bottom: 5px;
                    right: 5px;
                    width: 30px;
                    height: 30px;
                    background: ${statusColors[status]};
                    border: 4px solid #0a0514;
                    border-radius: 50%;
                    box-shadow: 0 0 10px ${statusColors[status]};
                `;
                avatar.parentElement.appendChild(statusDot);
            }
        }


        fetchDiscordProfile();


        const starsContainer = document.getElementById('stars');
        for (let i = 0; i < 100; i++) {
            const star = document.createElement('div');
            star.className = 'star';
            star.style.left = Math.random() * 100 + '%';
            star.style.top = Math.random() * 100 + '%';
            star.style.animationDelay = Math.random() * 3 + 's';
            starsContainer.appendChild(star);
        }

    </script>

